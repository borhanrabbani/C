#include <stdio.h>
#include <string.h>

int main(){
    char str[100];
    int n,i,j;
    gets(str);
    scanf("%d", &n);
    int len=strlen(str);
    for(i=0; i<len-n; i++)
    {
        for(j=0; j<4; j++)
            printf("%c", str[i+j]);
        printf("\n");
        //printf("%c\n", str[i]);
    }
}
